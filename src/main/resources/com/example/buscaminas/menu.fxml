<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="TOP_CENTER" spacing="10.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.buscaminas.Controllers.MenuController">

    <Label text="BUSCAMINAS" style="-fx-font-weight: bold; -fx-font-size: 20"/>

    <Button text="Fácil" onAction="#iniciarFacil"/>
    <Button text="Intermedio" onAction="#iniciarIntermedio"/>
    <Button text="Difícil" onAction="#iniciarDificil"/>
    <Button text="Salir" onAction="#salir"/>

</VBox>
